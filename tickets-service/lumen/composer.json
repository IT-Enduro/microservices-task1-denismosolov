{
    "name": "denismosolov/tickets-service",
    "description": "Tickets Service.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "flipbox/lumen-generator": "^9.2",
        "guzzlehttp/guzzle": "^7.6",
        "laravel/lumen-framework": "^10.0",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
